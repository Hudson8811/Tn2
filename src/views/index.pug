extends layouts/default
include helpers/mixins

block variables
	- const title = "Project title"

block content
	.section.start-banner__section 
		.start-banner
			.start-banner__picture
				img(src="img/start-banner.png", alt="")
			.start-banner__desc
				.container
					.start-banner__desc__title
						!="Клей-пена <br> для sip панелей" 
					.btn Подробнее
		
	.section.heat-loss__section
		.heat-loss
			.heat-loss__col
				.heat-loss__picture 
					img(src="img/heat-loss__1.png", alt="")
			.heat-loss__col
				.heat-loss__row
					.heat-loss__title.title__34
						!="<span>Проблема</span> - теплопотери в зданиях из sip"
					.heat-loss__text.text__24
						!="Зазор 2 - 7 мм в местах соединения панелей."
					.heat-loss__text.text__24
						!="Здесь происходят основные теплопотери с которыми не справляются стандартные монтажные пены"
				.heat-loss__row.heat-loss__row--black.heat-loss__balloon--col
					.heat-loss__title.title__34.text__white
						!="<span>Решение</span> <br>Клей-пена технониколь для sip"
					.heat-loss__text.text__24.text__white
						!="Продукт, созданный специально для работы с узкими швами при строительстве зданий по технологии СИП"
					.heat-loss__balloon
						img(src="img/balloon.png", alt="")
	
	.section.heat-loss__section
		.heat-loss
			.heat-loss__col
				.heat-loss__row.heat-loss__row--black
					.application__title.title__34.text__white
						!="Применение"
					.application__text.text__24.text__white
						!="Качество строительства напрямую зависит от правильного выбора продуктов"
					.heat-loss__advantages
						.heat-loss__advantage
							.heat-loss__advantage__icon
								img(src="img/heat-loss__advantage__icon--1.png", alt="")
							.heat-loss__advantage__title.title__30.text__white Герметизация
							.heat-loss__advantage__text.text__24.text__white Соединений, пустот, полостей
						.heat-loss__advantage
							.heat-loss__advantage__icon
								img(src="img/heat-loss__advantage__icon--2.png", alt="")
							.heat-loss__advantage__title.title__30.text__white Монтаж sip панелей
							.heat-loss__advantage__text.text__24.text__white При строительстве зданий и сооружений
				.heat-loss__row.heat-loss__row--border
					.heat-loss__absence
						.heat-loss__absence__icon
							img(src="img/heat-loss__advantage__icon--3.png", alt="")
						.heat-loss__absence__text.text__24
							!="Отсутствие брака при монтаже - клей создает прочное, герметичное соединение панелей SIP"
				
			.heat-loss__col
				.heat-loss__picture 
					img(src="img/heat-loss__2.png", alt="")
	
	.section.advantages__section
		.container
			.advantages__title.title__34 Преимущества клей-пены технониколь для sip
			.advantages
				.advantages__col
					.advantage
						.advantage__col
							.advantage__icon 
								img(src="img/advantage__icon__1.png", alt="")
							.advantage__value
								.advantage__value__title.text__24 Оптимальное вторичное расширение 
								.advantage__value__text.text__24 Эффективно дозаполняет пустоты и неровнсти основания
							.advantage__arrow 
								img(src="img/advantage__arrow.png", alt="")
						.advantage__col
							.advantage__value
								.advantage__value__title.text__24 Теплое соединение
								.advantage__value__text.text__24 Без мостиков холода	
					.advantage
						.advantage__col
							.advantage__icon 
								img(src="img/advantage__icon__2.png", alt="")
							.advantage__value
								.advantage__value__title.text__24 Упругость
								.advantage__value__text.text__24 Работает вместе с конструкцией
							.advantage__arrow 
								img(src="img/advantage__arrow.png", alt="")
						.advantage__col
							.advantage__value
								.advantage__value__title.text__24 Долговечная герметизация
								.advantage__value__text.text__24 Соединения	
					.advantage
						.advantage__col
							.advantage__icon 
								img(src="img/advantage__icon__3.png", alt="")
							.advantage__value
								.advantage__value__title.text__24  Экономичный расход
								.advantage__value__text.title__32 1 баллон = 5 панелей
							.advantage__arrow 
								img(src="img/advantage__arrow.png", alt="")
						.advantage__col
							.advantage__value
								.advantage__value__title.text__24
									!="Производительность выше на <span>20%</span>"
								.advantage__value__text.text__24 По сравнению с обычной клей-пеной
				.advantages__col
					.all-season
						.all-season__title.text__24 Всесезонный
						.all-season__picture
							img(src="img/all-season__picture.png", alt="")
	.section.questions__section
		.questions
			.accordions
				.accordion
					.container
						.accordion__intro
							p Документация
